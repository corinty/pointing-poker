{
  "name": "pointing-poker",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "db:setup": "npm run db:push && npm run db:seed",
    "db:seed": "node --import tsx --env-file .env ./app/db/drizzle/seed.ts",
    "db:reset": "node --import tsx --env-file .env ./app/db/drizzle/reset.ts && npm run db:push && npm run  db:seed",
    "db:push": "drizzle-kit push:sqlite",
    "db:studio": "drizzle-kit studio --verbose",
    "dev": "remix dev --manual",
    "lint": "eslint --ignore,-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:formatting": "prettier --write app/**",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
